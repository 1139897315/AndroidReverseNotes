<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_selected="true">
        <shape android:shape="rectangle" android:useLevel="true">
            <solid android:color="@color/e1"/>
            <corners android:topRightRadius="@dimen/qt" android:bottomRightRadius="@dimen/qt"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle" android:useLevel="true">
            <solid android:color="@color/e1"/>
            <corners android:topRightRadius="@dimen/qt" android:bottomRightRadius="@dimen/qt"/>
        </shape>
    </item>
    <item android:state_focused="false" android:state_pressed="true">
        <shape android:shape="rectangle" android:useLevel="true">
            <solid android:color="@color/e1"/>
            <corners android:topRightRadius="@dimen/qt" android:bottomRightRadius="@dimen/qt"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle" android:useLevel="true">
            <solid android:color="@color/a3p"/>
            <corners android:topRightRadius="@dimen/qt" android:bottomRightRadius="@dimen/qt"/>
        </shape>
    </item>
</selector>
